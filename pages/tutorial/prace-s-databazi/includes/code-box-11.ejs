    <pre data-code="KontaktDBUtil.java" data-java-package data-java-package-opened data-active><code class="language-java">import java.sql.Connection;
import java.sql.SQLException;

import javax.sql.DataSource;

public class KontaktDBUtil {
    private Connection conn;
    
    public KontaktDBUtil(DataSource dataSource) {
        try {
            conn = dataSource.getConnection();
        } catch (SQLException e) {
            System.out.println(e);
        }
    }
    
    public void close() {
        try {
            conn.close();
        } catch(SQLException e) {
            System.out.println(e);
        }
    }
